# -----------------------------------------------------
# ALIASES
# -----------------------------------------------------

# -----------------------------------------------------
# Snork
# -----------------------------------------------------

# zoxide
alias cd="z"

# bat
alias cat="bat --color=always --line-range :500"

# zsh
alias zshconfig="nvim ~/dotfiles"
alias zshreload="source ~/dotfiles/.zshrc"

# kitty
alias kssh="kitten ssh"

# Python 
alias pyactivate="source .venv/bin/activate"

